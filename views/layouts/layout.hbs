          content="width=device-width, initial-scale=1.0, maximum-scale=5.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="keywords" content="{{{ page.pageKeywords }}}">
    <meta name="description" content="{{{ page.pageDescription }}}">
    <link rel="stylesheet" href="/public/styles/main.min.css">
    <title>{{{ page.pageTitle }}}</title>
    <link rel="apple-touch-icon" type="image/png" href="/public/images/favicon-192x192.png" sizes="192x192" />
