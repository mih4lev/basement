<body>

{{> header/header }}

<main>
    {{{ body }}}
</main>

{{> footer/footer }}

{{> modals/view-idea/view-idea.modal }}
{{> modals/save-idea/save-idea.modal }}
{{> modals/sign-in/sign-in.modal }}
