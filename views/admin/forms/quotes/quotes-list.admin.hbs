<ul class="formsData">
    {{# each forms }}
        <li class="dataWrapper">
            <a class="dataLink" href="{{{ requestID }}}">
                <span class="fieldName">{{{ firstName }}} {{{ lastName }}}</span>
                <span class="fieldConsultation">{{{ square }}} SQ. FT. / {{{ zipCode }}}</span>
                <span class="fieldEmail">{{{ email }}}</span>
                <span class="timestamp">{{{ timestamp }}}</span>
            </a>
        </li>
    {{/ each }}
</ul>